<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../../DetectorDescription/Schema/DDLSchema.xsd">

<ConstantsSection label="tibstringpar.xml" eval="true">
 <Constant name="StringL"        value="140.20*cm"/>
 <Constant name="MotherCableW"   value="2.40*cm"/>
 <Constant name="MotherCableT"   value="0.06*cm"/>
 <Constant name="MotherCableY"   value="0.11*cm"/>
 <Constant name="CoolTubeD"      value="2.8*mm"/>
 <Constant name="CoolTubeSep"    value="5.40*cm"/>
 <Constant name="ICCBoxW"        value="6.54*cm"/>
 <Constant name="ICCBoxT"        value="3.00*mm"/>
 <Constant name="ICCBoxL"        value="3.50*cm"/>
 <Constant name="ICCW"           value="6.54*cm"/>
 <Constant name="ICCT"           value="0.19*cm"/>
 <Constant name="ICCL"           value="3.50*cm"/>
 <Constant name="ICCY"           value="0.055*cm"/>
 <Constant name="ICCCool1W"      value="0.625*cm"/>
 <Constant name="ICCCool2W"      value="4.75*cm"/>
 <Constant name="ICCCoolT"       value="0.11*cm"/>
 <Constant name="ICCCool1L"      value="3.50*cm"/>
 <Constant name="ICCCool2L"      value="0.30*cm"/>
 <Constant name="ICCCoolX"       value="2.6875*cm"/>
 <Constant name="ICCCoolY"       value="-0.095*cm"/>
 <Constant name="ICCCoolZ"       value="1.60*cm"/>
 <Constant name="ICCBoxToMod"    value="6.0*mm"/>
 <Constant name="CCUML"          value="2.20*cm"/>
 <Constant name="CCUMZ"          value="69.0*cm"/>
 <Constant name="CoolTubeW"      value="[CoolTubeD]+[CoolTubeSep]"/>
 <Constant name="SideCutDx"      value="[tibmodpar:DSModuleSideCutDx]"/>
 <Constant name="SideCutX"       value="[tibmodpar:ModuleDx]-[SideCutDx]+0.3*mm"/>
</ConstantsSection>

<SolidSection label="tibstringpar.xml">
 <Box name="TIBSSString1"               dx="[tibmodpar:ModuleDx]"    
      dy="[tibmodpar:SSModuleDy]"       dz="[tibstringpar:StringL]/2"/>

 <Box name="TIBDSString1MainPart"       dx="[tibmodpar:ModuleDx]"    
      dy="[tibmodpar:DSModuleDy]"       dz="[tibstringpar:StringL]/2"/>
 <Box name="TIBDSString1SideCutL"       dx="[SideCutDx]"    
      dy="[tibmodpar:DSModuleSideCutDy]"   dz="[tibstringpar:StringL]/2+[tibmodpar:SideCutExtraZ]"/>
 <Box name="TIBDSString1SideCutR"       dx="[SideCutDx]"    
      dy="[tibmodpar:DSModuleSideCutDy]"   dz="[tibstringpar:StringL]/2+[tibmodpar:SideCutExtraZ]"/>
 <SubtractionSolid name="TIBDSString1LeftCutted">
   <rSolid name="TIBDSString1MainPart"/>
   <rSolid name="TIBDSString1SideCutL"/>
   <Translation x="-[SideCutX]" y="[tibmodpar:DSModuleSideCutY]" z="0."/>
 </SubtractionSolid>
 <SubtractionSolid name="TIBDSString1">
   <rSolid name="TIBDSString1LeftCutted"/>
   <rSolid name="TIBDSString1SideCutR"/>
   <Translation x="[SideCutX]" y="[tibmodpar:DSModuleSideCutY]" z="0."/>
 </SubtractionSolid>

 <Box name="TIBString2"               dx="[tibstringpar:CoolTubeW]/2"    
      dy="[tibstringpar:CoolTubeD]/2" dz="[tibstringpar:StringL]/2"/>
 <Box name="TIBICCBox"                dx="[tibstringpar:ICCBoxW]/2"    
      dy="[tibstringpar:ICCBoxT]/2"   dz="[tibstringpar:ICCBoxL]/2"/>
 <Box name="TIBICC"                   dx="[tibstringpar:ICCW]/2"    
      dy="[tibstringpar:ICCT]/2"      dz="[tibstringpar:ICCL]/2"/>
 <Box name="TIBICCCool1"              dx="[tibstringpar:ICCCool1W]/2"    
      dy="[tibstringpar:ICCCoolT]/2"  dz="[tibstringpar:ICCCool1L]/2"/>
 <Box name="TIBICCCool2"              dx="[tibstringpar:ICCCool2W]/2"    
      dy="[tibstringpar:ICCCoolT]/2"  dz="[tibstringpar:ICCCool2L]/2"/>
 <Box name="TIBCCUM"                  dx="[tibstringpar:ICCBoxW]/2"    
      dy="[tibstringpar:ICCBoxT]/2"   dz="[tibstringpar:CCUML]/2"/>
 </SolidSection>

<LogicalPartSection label="tibstringpar.xml">
 <LogicalPart name="TIBICCCool1" category="unspecified">
  <rSolid name="TIBICCCool1"/>
  <rMaterial name="tibmaterial:TIB_ICC_ledge1"/>
 </LogicalPart>
 <LogicalPart name="TIBICCCool2" category="unspecified">
  <rSolid name="TIBICCCool2"/>
  <rMaterial name="tibmaterial:TIB_ICC_ledge2"/>
 </LogicalPart>
 <LogicalPart name="TIBCCUM" category="unspecified">
  <rSolid name="TIBCCUM"/>
  <rMaterial name="tibtidcommonmaterial:TIBTID_CCUM"/>
 </LogicalPart>
</LogicalPartSection>

<RotationSection label="tibstringpar.xml">
  <Rotation name="180D" thetaX="90*deg" phiX="180*deg"   thetaY="90*deg" 
                        phiY="90*deg"   thetaZ="180*deg" phiZ="0*deg" />
  <Rotation name="90XD" thetaX="90*deg" phiX="90*deg"    thetaY="0*deg" 
                        phiY="0*deg"    thetaZ="90*deg"  phiZ="0*deg" />
</RotationSection>

</DDDefinition>
